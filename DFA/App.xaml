<Application x:Class="DFA.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:DFA"
             StartupUri="Windows/MainWindow.xaml">
    <Application.Resources>

        <Color x:Key="DefaultBgDark">#FF141414</Color>
        <Color x:Key="DefaultGreen">#FFB2FF59</Color>
        <Color x:Key="DefaultRed">#FFDD2C00</Color>
        
        <Color x:Key="DefaultForeground">#FFBB8FCE</Color>
    </Application.Resources>
</Application>
